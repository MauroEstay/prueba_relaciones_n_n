<div class="container">
  <div class="row">
    <div class="container">
      <table class="table">
        <h2>Tareas realizadas</h2>
        <thead>
          <tr>
            <th scope="col">Tarea</th>
            <th scope="col">Fecha</th>
          </tr>
        </thead>
        <tbody>
          <% @usertasks.each do |usertask| %>
            <tr>
              <td><%= usertask.task.name %></td>>
              <td><%= time_ago_in_words(usertask.created_at) %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
